--- a/src/xdocs/config_imports.xml
+++ b/src/xdocs/config_imports.xml
@@ -2359,6 +2359,19 @@ public class SomeClass { }
         <source>
 &lt;module name=&quot;RedundantImport&quot;/&gt;
         </source>
